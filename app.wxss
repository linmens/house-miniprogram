@import "miniprogram_npm/tdesign-miniprogram/common/style/theme/_index.wxss";

page {
  background-color: var(--td-bg-color-page);
}

.m-l-20 {
  margin-left: 20rpx;
}
.m-t-20 {
  margin-top: 20rpx;
}
.f-24 {
  font-size: 24rpx;
}

.p-20 {
  padding: 20rpx;
}

.px-20 {
  padding: 0 20rpx;
}

.p-b-20 {
  padding-bottom: 20rpx;
}

.flex {
  display: flex;
}

.flex-1 {
  flex: 1;
}

.f-row-center {
  justify-content: center;
}

.f-row-between {
  justify-content: space-between;
}

.f-col-center {
  align-items: center;
}

.text-center {
  text-align: center;
}

.text-right {
  text-align: right;
}
.t-tab-bar--placeholder {
  height: var(--td-tab-bar-height, 112rpx);
  box-sizing: border-box;
  user-select: none;
  padding: 0 24rpx;
  position: relative;
}

.t-tab-bar {
  z-index: 99;
}

.t-tab-bar-safe {
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  flex: 1;
}
