@import "miniprogram_npm/tdesign-miniprogram/common/style/theme/_index.wxss";

page {
  --margin-32: 32rpx;
  --white: 255, 255, 255;
  --black: 0, 0, 0;
  --global-bg-img: url(https://p-pc-weboff.byteimg.com/tos-cn-i-9r5gewecjs/test.png);
  --td-navbar-bg-color: transparent;
  --td-brand-color: rgb(254, 44, 85);
  --td-brand-color-active: #d21b46;
  /* --td-bg-color-page */
  --neutral-50: 249, 249, 250;
  --neutral-100: 242, 242, 244;
  --neutral-800: 51, 52, 63;
  --neutral-900: 37, 38, 50;
  --neutral-950: 22, 24, 35;
  --color-fill-hover: rgba(var(--neutral-900), 0.08);
  --color-bg-b0: rgba(var(--neutral-50), 1);
  --color-bg-nav: rgba(var(--neutral-100), 1);
  --color-bg-b2: rgba(var(--neutral-100), 1);
  --color-text-t0: rgba(var(--black), 1);
  --color-text-t2: rgba(var(--neutral-950), 0.75);
  --color-text-t3: rgba(var(--neutral-950), 0.6);
  --color-line-l3: rgba(var(--neutral-950), 0.06);
  --color-bg-b1-white: rgba(var(--white), 1);
  --color-secondary-default: rgba(var(--neutral-100), 1);
  --semi-color-text-2: rgba(var(--semi-grey-9), 0.6);
  --semi-grey-9: 35, 28, 31;
  background: no-repeat var(--global-bg-img) var(--color-bg-b0);
  /* background-color: var(--td-bg-color-page); */
  background-color: var(--color-bg-b0);
  --td-tab-bar-active-bg: var(--color-fill-hover);
  --td-tab-bar-color: var(--color-text-t3);
  --td-tab-bar-active-color: var(--color-text-t0);
  --td-tab-bar-bg-color: var(--color-bg-b0);
  /* --td-tab-bar-round-shadow: ; */
  --td-grid-bg-color: var(--color-bg-b1-white);
  --td-grid-item-bg-color: var(--color-bg-b1-white);
  --td-grid-item-text-color: var(--color-text-t2);
  --td-input-bg-color: var(--color-bg-b1-white);
  --td-tab-nav-bg-color: var(--color-bg-b1-white);
  --td-collapse-panel-bg-color: var(--color-bg-b1-white);
  --td-cell-hover-color: var(--color-fill-hover);
  --td-cell-left-icon-color: var(--color-text-t3);
  --td-step-item-default-circle-bg: var(--color-bg-b1-white);
  --td-bg-color-container: var(--color-bg-b1-white);
  --td-button-default-bg-color: var(--color-bg-b2);
  --td-button-default-border-color: var(--color-bg-b2);
  --td-button-default-active-bg-color: var(--color-fill-hover);
  --td-button-default-active-border-color: var(--color-bg-b2);
  --td-notice-bar-info-bg-color: transparent;
  --td-notice-bar-font-color: var(--semi-color-text-2);
  --td-grid-item-hover-bg-color: var(--color-fill-hover);
  --td-footer-link-color: var(--color-text-t3);
  --td-guide-popover-border: 0.5px solid var(--color-line-l3);
}
@media (prefers-color-scheme: dark) {
  page,
  .page {
    --semi-grey-9: 249, 249, 249;
    --color-fill-hover: rgba(var(--neutral-100), 0.08);
    --color-text-t0: rgba(var(--white), 1);
    --color-text-t1: rgba(var(--white), 0.9);
    --color-text-t2: rgba(var(--white), 0.75);
    --color-text-t3: rgba(var(--white), 0.6);
    --td-text-card-color: rgb(194, 215, 239);
    --color-line-l3: rgba(var(--white), 0.04);
    --color-bg-b0: rgba(var(--neutral-950), 1);
    --color-bg-b1-white: rgba(var(--neutral-900), 1);
    --color-bg-b2: rgba(var(--neutral-800), 1);
    --color-secondary-default: rgba(var(--neutral-100), 0.08);
    --semi-color-text-2: rgba(var(--semi-grey-9), 0.6);
    --td-tab-bar-active-bg: var(--color-fill-hover);
    --td-tab-bar-bg-color: var(--color-bg-b1-white);
    /* --td-tab-bar-round-shadow: none; */
    --td-cell-bg-color: var(--color-bg-b1-white);
    --td-notice-bar-info-bg-color: transparent;
    --td-bg-color-secondarycontainer: var(--color-secondary-default);
    --td-guide-popover-border: 0.5px solid var(--color-line-l3);
  }
}
.t-tag--light.t-tag--default {
  border-color: transparent;
}
.t-navbar-class {
  background: no-repeat var(--global-bg-img) var(--color-bg-b0);
}
.bg-main {
  background-color: var(
    --td-navbar-bg-color,
    var(--td-bg-color-container, var(--td-font-white-1, #ffffff))
  );
}
.calc-bottom-btn {
  display: flex;
  margin: 0 32rpx;
  position: sticky;
  bottom: calc(env(safe-area-inset-bottom) + 32rpx);
  z-index: 99;
}
.m-l-20 {
  margin-left: 20rpx;
}
.m-t-20 {
  margin-top: 20rpx;
}
.m-b-20 {
  margin-bottom: 20rpx;
}
.my-20 {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.f-24 {
  font-size: 24rpx;
}

.p-20 {
  padding: 20rpx;
}

.px-20 {
  padding: 0 20rpx;
}

.p-b-20 {
  padding-bottom: 20rpx;
}
.py-20 {
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.flex {
  display: flex;
}

.flex-1 {
  flex: 1;
}
.font-20 {
  font-size: 20rpx;
}
.f-row-center {
  justify-content: center;
}

.f-row-between {
  justify-content: space-between;
}

.f-col-center {
  align-items: center;
}

.text-center {
  text-align: center;
}

.text-right {
  text-align: right;
}
.text-blod {
  font-weight: bold;
}
.t-tab-bar--placeholder {
  height: var(--td-tab-bar-height, 112rpx);
  box-sizing: border-box;
  user-select: none;
  padding: 0 24rpx;
  position: relative;
}

.t-tab-bar {
  z-index: 99;
}

.t-tab-bar-safe {
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  flex: 1;
}
