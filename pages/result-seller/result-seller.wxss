.seller-notice {
  font-size: 24rpx;
  margin: 0 32rpx;
  color: var(
    --td-footer-text-color,
    var(--td-text-color-placeholder, var(--td-font-gray-3, rgba(0, 0, 0, 0.4)))
  );
  line-height: var(--td-footer-text-line-height, 40rpx);
}
.row-tag {
  display: flex;
  align-items: center;
  overflow: hidden;
  position: relative;
  padding-bottom: 32rpx;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
}
.seller-inset-card {
  background-color: var(--color-secondary-default);
  padding: 10rpx;
  margin: 10rpx 0;
  border-radius: 10rpx;
}
.seller-inset-card__header {
  border-bottom: 1rpx solid var(--color-line-l3);
  padding-bottom: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.bank-total {
  margin: 32rpx;
  align-items: center;
}
.bank-total__item {
  display: flex;
  flex-direction: column;
  flex: 1;
  padding: 8rpx;
  background-color: var(--color-bg-b1-white);
  border-radius: 10rpx;
  box-sizing: border-box;
  overflow: hidden;
  box-shadow: 0 0 0.5px 0 var(--color-secondary-default);
  border: 0.5px solid var(--color-line-l3);
}
.bank-total__item__label {
  font-size: 20rpx;
  color: var(--color-text-t2);
  width: 100%;
}
.bank-total__item__value {
  color: var(--color-text-t0);
}
.bank-total__item__desc {
  font-size: 20rpx;
  color: var(--td-warning-color-8);
}
